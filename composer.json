{
    "name": "takutakuaoao/php-cs-fixer-align-property-rule",
    "description": "the cs-fixer-rule aligning vertical positions of class properties.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpCsFixerAlignPropertyRule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Units\\": "tests/units",
            "Tests\\Fixtures\\": "tests/_fixtures"
        }
    },
    "authors": [
        {
            "name": "takuya.aotsuka"
        }
    ],
    "require": {
        "php": ">=8.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.12",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit tests --colors=always"
        ]
    }
}
